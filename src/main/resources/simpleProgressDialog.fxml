<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane prefHeight="100.0" prefWidth="300.0" stylesheets="/layout/main.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
    <GridPane prefHeight="100.0" prefWidth="300.0">
        <rowConstraints>
            <RowConstraints percentHeight="50" />
            <RowConstraints percentHeight="50" />
        </rowConstraints>
        <columnConstraints>
            <ColumnConstraints percentWidth="100" />
        </columnConstraints>

        <Label fx:id="label" text="Message" GridPane.rowIndex="0">
         <GridPane.margin>
            <Insets left="10.0" />
         </GridPane.margin></Label>
        <ProgressBar fx:id="progressBar" prefHeight="18.0" prefWidth="300.0" style=" -fx-indeterminate-bar-animation-time: 1.0;-fx-accent: yellow;" GridPane.rowIndex="1">
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding></ProgressBar>
    </GridPane>
</AnchorPane>
